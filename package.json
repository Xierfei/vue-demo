{
  "name": "pg-vue",
  "version": "0.0.0",
  "homepage": "https://pg-vue-css.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-vue.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "now": "npm run build && npm run preview",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write . '!public'"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "pinia": "^2.0.35",
    "vue": "^3.3.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.64",
    "@pinegrow/vite-plugin": "^3.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.3",
    "@unocss/preset-icons": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/devtools": "^6.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "unocss": "^0.51.12",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.3.9",
    "vite-plugin-vue-devtools": "^0.0.20",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vue-tsc": "^1.6.4"
  },
  "web-types": "web-types.json"
}
